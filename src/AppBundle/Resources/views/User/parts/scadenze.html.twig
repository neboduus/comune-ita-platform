<div class="panel panel-primary no-border-radius">
    <div class="panel-heading">
        <h3 class="panel-title">{{ 'user.dashboard.scadenze'|trans }}</h3>
    </div>
    <div class="panel-body">
        <ul class="list-unstyled list-deadlines">
        </ul>
    </div>
</div>

{% verbatim %}
<script id="tpl-deadlines-spinner" type="text/x-jsrender">
<li style="padding:50px;text-align:center;border:none">
    <i class="fa fa-circle-o-notch fa-spin fa-3x fa-fw"></i>
    <span class="sr-only">Loading...</span>
</li>
</script>
<script id="tpl-deadlines" type="text/x-jsrender">
    <li>
        <span class="date small">{{:date}}</span>
        <a href="#" class="small">{{:title}}</a>
    </li>
</script>
<script id="tpl-deadlines-empty" type="text/x-jsrender">
<li style="padding:50px;text-align:center;border:none">
    <i class="fa fa-check fa-3x"></i>
</li>
</script>
{% endverbatim %}