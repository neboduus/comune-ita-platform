{% extends '@App/Default/index.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}

{% endblock %}

{% block main_content %}

    <div class="row">
        <div class="col-md-6">
            <h3>{{ 'operatori.usage.service_title'|trans }}</h3>
            <p><span class="badge">{{ servizi }}</span></p>
        </div>
        <div class="col-md-6">
            <h3>{{ 'operatori.usage.pratiche_title'|trans }}</h3>
            {% for year, count in pratiche %}
                <p>
                    <strong>{{ year }}:</strong> <span class="badge">{{ count }}</span>
                </p>
            {% endfor %}
        </div>
    </div>


{% endblock %}