      "calculateValue": "if(_.has(data, 'fiscal_code')){\n    let name = data.completename.data.name;\n    let surname = data.completename.data.surname;\n    let gender = data.gender.data.gender;\n    let date_of_birth = data.Born.data.natoAIl;\n    let fiscal_code = data.fiscal_code && data.fiscal_code.data.fiscal_code && data.fiscal_code.data.fiscal_code !== '' ? data.fiscal_code.data.fiscal_code.replace(/_/gi, \"\") : '';\n    \n    function loadJSON(path, payload, success, error) {\n        var xhr = new XMLHttpRequest();\n        xhr.open(\"POST\", path, true);\n        xhr.setRequestHeader(\"Content-Type\", \"application/json;charset=UTF-8\");\n    \n        xhr.onreadystatechange = function () {\n            if (xhr.readyState === XMLHttpRequest.DONE) {\n              var response = JSON.parse(xhr.response)\n              if (xhr.status === 200) {\n                document.getElementById('cf_alert').style.display = 'none';\n              } else {\n                document.getElementById('cf_alert').style.display = 'block';\n              }\n            }\n        };\n        \n        xhr.send(JSON.stringify(payload));\n    }\n    \n    if (name && surname && gender && date_of_birth && fiscal_code && fiscal_code.length === 16) {\n        let payload = {\n          \"name\": name,\n          \"surname\": surname,\n          \"gender\": gender,\n          \"date_of_birth\": date_of_birth,\n          \"fiscal_code\": fiscal_code.toUpperCase()\n        }\n    \n        loadJSON('https://fiscalcode.opencontent.it/validate', payload,\n            function () {\n            },\n            function () {\n            });\n    }\n}",
