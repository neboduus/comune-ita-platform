:80

respond /health status 200
root * /srv

file_server browse
metrics /metrics

@assets {
    path "*.js"
    path "*.css"
}

header @assets {
    Cache-Control "max-age=31536000"
}

log {
  format console
}
php_fastcgi * php:9000 {
  split .php
  root /var/www/html/public
  trusted_proxies private_ranges
}

encode gzip zstd
file_server
