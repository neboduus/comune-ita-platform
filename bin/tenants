#!/bin/bash

readonly env=${SYMFONY_ENV:-prod}

if [[ ! -d app/config ]]; then
  echo "Error, the script must be executed from root app dir"
  exit 1
fi

readonly tenants=$(ls -1d app/config/*/ | egrep -v '(bundles|common)' | cut -d'/' -f 3)

for tenant in $tenants; do
  if [[ -f app/config/${tenant}/config_${env}.yml ]]; then
     echo $tenant
  fi
done
