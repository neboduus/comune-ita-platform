{
  "name": "opencontent/stanzadelcittadino",
  "license": "GPL-3.0-only",
  "type": "project",
  "require": {
    "php": ">=7.3.0",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "anyx/login-gate-bundle": "*",
    "craue/formflow-bundle": "*",
    "doctrine/data-fixtures": "*",
    "doctrine/doctrine-bundle": "*",
    "doctrine/doctrine-cache-bundle": "*",
    "doctrine/doctrine-fixtures-bundle": "*",
    "doctrine/doctrine-migrations-bundle": "*",
    "doctrine/orm": "*",
    "eightpoints/guzzle-bundle": "*",
    "excelwebzone/recaptcha-bundle": "*",
    "flagception/flagception-bundle": "*",
    "friendsofsymfony/rest-bundle": "*",
    "gedmo/doctrine-extensions": "*",
    "h4cc/wkhtmltopdf-amd64": "*",
    "incenteev/composer-parameter-handler": "*",
    "jms/serializer-bundle": "*",
    "johngrogg/ics-parser": "*",
    "knplabs/knp-snappy-bundle": "*",
    "league/csv": "*",
    "lexik/jwt-authentication-bundle": "*",
    "nelmio/api-doc-bundle": "^3.7",
    "nesbot/carbon": "*",
    "omines/datatables-bundle": "*",
    "php-http/guzzle6-adapter": "*",
    "ramsey/uuid": "*",
    "sensio/framework-extra-bundle": "*",
    "sentry/sentry-symfony": "*",
    "stof/doctrine-extensions-bundle": "*",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "4.4.*",
    "symfony/flex": "^1.9",
    "symfony/monolog-bundle": "*",
    "symfony/polyfill-apcu": "*",
    "symfony/swiftmailer-bundle": "3.5.1",
    "symfony/translation": "*",
    "symfony/twig-bundle": "4.4.*",
    "symfony/webpack-encore-bundle": "^1.7",
    "thecodingmachine/gotenberg-php-client": "*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "vich/uploader-bundle": "^1.15"
  },
  "require-dev": {
    "asimlqt/php-google-spreadsheet-client": "3.0.2",
    "escapestudios/symfony2-coding-standard": "2.11.0",
    "phpspec/phpspec": "*",
    "phpstan/phpstan": "^0.12.50",
    "phpunit/phpunit": "7.5.20",
    "roave/security-advisories": "dev-master",
    "symfony/browser-kit": "^4.4",
    "symfony/css-selector": "^4.4",
    "symfony/debug-bundle": "^4.4",
    "symfony/dotenv": "^4.0",
    "symfony/maker-bundle": "^1.20",
    "symfony/phpunit-bridge": "^5.1",
    "symfony/stopwatch": "^4.4",
    "symfony/var-dumper": "^4.4",
    "symfony/web-profiler-bundle": "^4.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  }
}
