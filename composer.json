{
  "name": "opencontent/stanzadelcittadino",
  "license": "GPL-3.0-only",
  "type": "project",
  "description": "The citizen's personal area, designed with the Italian Digital Team using the starter KIT for a municipality site.",
  "version": "2.0.0-rc.9",
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3.0",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "anyx/login-gate-bundle": "2.0.3",
    "artprima/prometheus-metrics-bundle": "*",
    "craue/formflow-bundle": "*",
    "davidepastore/codice-fiscale": "^0.9.0",
    "doctrine/data-fixtures": "*",
    "doctrine/doctrine-bundle": "*",
    "doctrine/doctrine-fixtures-bundle": "*",
    "doctrine/doctrine-migrations-bundle": "*",
    "doctrine/orm": "*",
    "eightpoints/guzzle-bundle": "*",
    "excelwebzone/recaptcha-bundle": "*",
    "exercise/htmlpurifier-bundle": "*",
    "flagception/flagception-bundle": "*",
    "friendsofsymfony/rest-bundle": "*",
    "gedmo/doctrine-extensions": "*",
    "h4cc/wkhtmltopdf-amd64": "*",
    "incenteev/composer-parameter-handler": "*",
    "jms/serializer-bundle": "*",
    "johngrogg/ics-parser": "*",
    "league/csv": "*",
    "league/flysystem-aws-s3-v3": "*",
    "league/html-to-markdown": "*",
    "lexik/jwt-authentication-bundle": "*",
    "nelmio/api-doc-bundle": "^3.7",
    "nesbot/carbon": "*",
    "omines/datatables-bundle": "*",
    "oneup/flysystem-bundle": "*",
    "php-http/guzzle6-adapter": "*",
    "queueit/knownuserv3": "*",
    "ramsey/uuid": "*",
    "sensio/framework-extra-bundle": "*",
    "sentry/sentry-symfony": "3.5.2",
    "stof/doctrine-extensions-bundle": "*",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/flex": "^1.9",
    "symfony/framework-bundle": "*",
    "symfony/yaml": "*",
    "symfony/monolog-bundle": "*",
    "symfony/polyfill-apcu": "*",
    "symfony/swiftmailer-bundle": "*",
    "symfony/translation": "*",
    "symfony/twig-bundle": "*",
    "symfony/webpack-encore-bundle": "^1.7",
    "thecodingmachine/gotenberg-php-client": "*",
    "twig/extra-bundle": "*",
    "twig/twig": "*",
    "vich/uploader-bundle": "^1.15",
    "whichbrowser/parser": "*",
    "mhujer/breadcrumbs-bundle": "*",
    "willdurand/js-translation-bundle": "*"
  },
  "require-dev": {
    "symfony/dotenv": "*",
    "symfony/phpunit-bridge": "^4.0",
    "phpspec/phpspec": "*",
    "phpunit/phpunit": "7.5.20",
    "squizlabs/php_codesniffer": "2.9.2",
    "escapestudios/symfony2-coding-standard": "2.11.0",
    "roave/security-advisories": "dev-master",
    "phpstan/phpstan": "1.7.12",
    "phpstan/phpstan-doctrine": "1.3.9",
    "phpstan/phpstan-phpunit": "1.1.1",
    "phpstan/phpstan-symfony": "1.2.5",
    "rector/rector": "0.13.4",
    "symfony/browser-kit": "*",
    "symfony/css-selector": "*",
    "symfony/debug-bundle": "*",
    "symfony/maker-bundle": "*",
    "symfony/stopwatch": "*",
    "symfony/var-dumper": "*",
    "symfony/web-profiler-bundle": "*"
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "4.4.*",
      "docker": true
    }
  }
}
