{
  "name": "opencontent/stanzadelcittadino",
  "license": "AGPL-3.0-only",
  "type": "project",
  "description": "The citizen's personal area, designed with the Italian Digital Team using the starter KIT for a municipality site.",
  "authors": [
    {
      "name": "Opencity Labs S.r.l.",
      "email": "info@opencitylabs.it",
      "homepage": "https://opencitylabs.it/",
      "role": "Mantainer"
    }
  ],
  "version": "2.13.0-rc.1",
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4.0",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "anyx/login-gate-bundle": "2.0.3",
    "craue/formflow-bundle": "3.6.0",
    "davidepastore/codice-fiscale": "0.9.0",
    "doctrine/data-fixtures": "1.5.3",
    "doctrine/dbal": "2.13.9",
    "doctrine/doctrine-bundle": "2.7.1",
    "doctrine/doctrine-fixtures-bundle": "3.4.2",
    "doctrine/doctrine-migrations-bundle": "3.2.2",
    "doctrine/orm": "2.13.1",
    "eightpoints/guzzle-bundle": "8.3.0",
    "excelwebzone/recaptcha-bundle": "1.5.36",
    "exercise/htmlpurifier-bundle": "4.1.1",
    "flagception/flagception-bundle": "4.3.0",
    "friendsofsymfony/rest-bundle": "3.3.0",
    "gedmo/doctrine-extensions": "3.8.0",
    "gmostafa/php-graphql-client": "^1.13",
    "gotenberg/gotenberg-php": "^1.1",
    "h4cc/wkhtmltopdf-amd64": "0.12.4",
    "incenteev/composer-parameter-handler": "2.1.5",
    "jmikola/geojson": "^1.0",
    "jms/serializer-bundle": "4.0.2",
    "johngrogg/ics-parser": "3.2.0",
    "league/csv": "9.7.4",
    "league/flysystem-aws-s3-v3": "1.0.30",
    "league/html-to-markdown": "5.1.0",
    "lexik/jwt-authentication-bundle": "2.16.0",
    "mhujer/breadcrumbs-bundle": "1.5.8",
    "nelmio/api-doc-bundle": "4.9.0",
    "nesbot/carbon": "2.62.1",
    "oat-sa/bundle-health-check": "^2.2",
    "omines/datatables-bundle": "0.5.5",
    "oneup/flysystem-bundle": "3.7.1",
    "php-http/guzzle7-adapter": "1.0.0",
    "queueit/knownuserv3": "3.7.1",
    "ramsey/uuid": "4.2.3",
    "sensio/framework-extra-bundle": "6.2.9",
    "sentry/sentry-symfony": "4.5.0",
    "stof/doctrine-extensions-bundle": "1.7.1",
    "symfony/asset": "4.4.46",
    "symfony/console": "4.4.49",
    "symfony/flex": "1.19.4",
    "symfony/framework-bundle": "4.4.49",
    "symfony/monolog-bundle": "3.8.0",
    "symfony/polyfill-apcu": "1.27.0",
    "symfony/swiftmailer-bundle": "3.5.4",
    "symfony/translation": "4.4.47",
    "symfony/twig-bundle": "4.4.41",
    "symfony/webpack-encore-bundle": "1.16.1",
    "symfony/yaml": "4.4.45",
    "twig/extra-bundle": "3.4.0",
    "twig/twig": "3.4.3",
    "vich/uploader-bundle": "1.16.1",
    "whichbrowser/parser": "2.1.7",
    "willdurand/js-translation-bundle": "5.0.0"
  },
  "require-dev": {
    "symfony/dotenv": "4.4.37",
    "symfony/phpunit-bridge": "4.4.49",
    "phpspec/phpspec": "7.2.0",
    "phpunit/phpunit": "7.5.20",
    "squizlabs/php_codesniffer": "2.9.2",
    "escapestudios/symfony2-coding-standard": "2.11.0",
    "roave/security-advisories": "dev-master",
    "phpstan/phpstan": "1.9.14",
    "phpstan/phpstan-doctrine": "1.3.32",
    "phpstan/phpstan-phpunit": "1.3.3",
    "phpstan/phpstan-symfony": "1.2.20",
    "rector/rector": "0.14.5",
    "symfony/browser-kit": "4.4.44",
    "symfony/css-selector": "4.4.44",
    "symfony/debug-bundle": "4.4.37",
    "symfony/maker-bundle": "v1.39.1",
    "symfony/stopwatch": "4.4.46",
    "symfony/var-dumper": "4.4.47",
    "symfony/web-profiler-bundle": "4.4.47"
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "4.4.*",
      "docker": true
    }
  }
}
