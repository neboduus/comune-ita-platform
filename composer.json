{
  "name": "opencontent/stanzadelcittadino",
  "license": "GPL-3.0-only",
  "type": "project",
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/InstanceKernel.php",
      "app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3.0",
    "symfony/symfony": "4.*",
    "doctrine/orm": "*",
    "doctrine/doctrine-bundle": "*",
    "doctrine/doctrine-cache-bundle": "*",
    "doctrine/doctrine-fixtures-bundle": "*",
    "doctrine/data-fixtures": "*",
    "symfony/swiftmailer-bundle": "*",
    "symfony/monolog-bundle": "*",
    "symfony/polyfill-apcu": "*",
    "sensio/framework-extra-bundle": "*",
    "incenteev/composer-parameter-handler": "*",
    "ramsey/uuid": "*",
    "gedmo/doctrine-extensions": "*",
    "symfony/translation": "*",
    "craue/formflow-bundle": "*",
    "vich/uploader-bundle": "*",
    "knplabs/knp-snappy-bundle": "*",
    "jms/serializer-bundle": "*",
    "doctrine/doctrine-migrations-bundle": "*",
    "h4cc/wkhtmltopdf-amd64": "*",
    "eightpoints/guzzle-bundle": "*",
    "nesbot/carbon": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-json": "*",
    "symfony/webpack-encore-bundle": "*",
    "anyx/login-gate-bundle": "*",
    "xiidea/easy-audit": "*",
    "omines/datatables-bundle": "*",
    "friendsofsymfony/rest-bundle": "*",
    "nelmio/api-doc-bundle": "*",
    "lexik/jwt-authentication-bundle": "*",
    "thecodingmachine/gotenberg-php-client": "*",
    "php-http/guzzle6-adapter": "*",
    "excelwebzone/recaptcha-bundle": "*",
    "ext-fileinfo": "*",
    "johngrogg/ics-parser": "*",
    "league/csv": "*",
    "sentry/sentry-symfony": "*",
    "stof/doctrine-extensions-bundle": "*",
    "flagception/flagception-bundle": "*"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.0",
    "phpspec/phpspec": "*",
    "phpunit/phpunit": "7.5.20",
    "squizlabs/php_codesniffer": "*",
    "asimlqt/php-google-spreadsheet-client": "3.0.2",
    "escapestudios/symfony2-coding-standard": "2.11.0"
  },
  "scripts": {

  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "hard",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
